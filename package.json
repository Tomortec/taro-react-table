{
  "name": "taro-react-table",
  "version": "1.1.0",
  "description": "taro表格",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "license": "MIT",
  "keywords": [
    "taro",
    "table",
    "react",
    "mobile",
    "component",
    "taro-table",
    "taro-react-table"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "umd": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "dev:type": "tsc --project ./tsconfig.json --watch --incremental",
    "build:type": "rimraf dist && tsc --project ./tsconfig.json --declaration",
    "build:rollup": "rimraf dist && rollup --config ./rollup.config.js"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "qiuweikang",
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@tarojs/cli": "^3.4.4",
    "@tarojs/components": "^3.4.4",
    "@tarojs/mini-runner": "3.4.4",
    "@tarojs/react": "^3.4.4",
    "@tarojs/runtime": "^3.4.4",
    "@tarojs/taro": "^3.4.4",
    "@tarojs/webpack-runner": "^3.4.4",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-preset-taro": "3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.4.4",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "less": "^4.1.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "stylelint": "^14.4.0",
    "typescript": "^4.1.0"
  },
  "dependencies": {
    "classnames": "^2.3.1"
  },
  "bugs": {
    "url": "https://github.com/qiuweikangdev/taro-react-table/issues"
  },
  "homepage": "https://github.com/qiuweikangdev/taro-react-table#readme"
}
